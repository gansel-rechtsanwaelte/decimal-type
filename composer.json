{
    "name": "gansel/decimal",
    "description": "Decimal numbers and calculation",
    "license": "commercial",
    "type": "project",
    "authors": [
        {
            "name": "Oliver Hoff",
            "email": "hoff@gansel-rechtsanwaelte.de"
        }
    ],
    "autoload": {
        "psr-4": { "Gansel\\Decimal\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Gansel\\Decimal\\Test\\": "test/" }
    },
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "doctrine/dbal": "^2.5",
		"phpunit/phpunit" : "^6.0",
        "symfony/framework-bundle": "^3.3",
        "symfony/serializer": "^3.3"
    },
    "suggest": {
        "doctrine/dbal": "A database mapping type is provided",
        "symfony/framework-bundle": "An integration bundle is provided",
        "symfony/serializer": "A normalizer is provided"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "platform": {
            "php": "7.1"
        }
    },
    "repositories": [
    ],
    "extra": {
    }
}
